def credit(self, amount):
    self.balance += amount
    print("RS.", amount, "Was credited in account: ")
    print("Total Balance is: ", self.get_balance())

def debit(self, amount):
    self.balance -= amount
    print("RS.", amount, "Was debited in account: ")
    print("Total Balance is: ", self.get_balance())
    
def get_balance(self):
    return self.balance
